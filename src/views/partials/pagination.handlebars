  <nav aria-label="Page navigation example navbar-dark  bg-dark">
    <ul class="pagination  justify-content-center">
      {{#if data.hasPrevPage}}
      <li class="page-item"><a class="page-link" href="http://localhost:8080/?page={{data.prevPage}}" >Prev</a></li>
      {{else}}
      <li class="page-item disabled"><a class="page-link" href="#" >Prev</a></li>
      {{/if}}
      {{#each data.all_prev}}
      <li class="page-item"><a class="page-link" href="http://localhost:8080/?page={{this}}">{{this}}</a></li>
      {{/each}}
      <li class="page-item active"><a class="page-link" href="http://localhost:8080/?page={{data.page}}">{{data.page}}</a></li>
      {{#each data.all}}
      <li class="page-item"><a class="page-link" href="http://localhost:8080/?page={{this}}">{{this}}</a></li>
      {{/each}}
      {{#if data.hasNextPage}}
      <li class="page-item"><a class="page-link" href="http://localhost:8080/?page={{data.nextPage}}">Next</a></li>
      {{else}}
      <li class="page-item disabled"><a class="page-link" href="#">Next</a></li>
      {{/if}}
    </ul>
  </nav>